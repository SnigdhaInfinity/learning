<div class="login-body">
    <article class="container-circuit center-block">
		<section>
			
			<div class="tab-content tabs-login col-lg-12 col-md-12 col-sm-12 cols-xs-12">
				<div id="login-access" class="tab-pane fade active in">
					<h2><i class="glyphicon glyphicon-log-in"></i> Circuit Ordering </h2>						
					<form method="post" accept-charset="utf-8" autocomplete="off" role="form" class="form-horizontal">
						<div class="form-group ">

							<label class="form-group ">Location</label>
								<input type="text" class="form-control" name="Location" id="Location_value" 
									placeholder="Location" tabindex="1" value="" />
						</div>
						<div class="form-group ">

							<label class="form-group ">Type Of Circuit</label>
							<select class="form-control">
								<option value=''></option>
								<option value="L2 Vpn">L2 Vpn</option>
								<option value="L3 Vpn">L3 Vpn</option>
								
							</select>
						</div>
						<div class="form-group ">
							<label for="Bandwidth" >Bandwidth</label>
								<input type="Bandwidth" class="form-control" name="Bandwidth" id="Bandwidth"
									placeholder="Bandwidth" value="" tabindex="2" />
						</div>
						<div class="form-group ">

							<label for="Parent Vpn" >Parent Vpn</label>
							<select class="form-control" id="state">
								<option value=''></option>
								<option value="L2 Vpn">L2 Vpn</option>
								<option value="L3 Vpn">L3 Vpn</option>
								 <option value="other">Other</option>
								
							</select>
						</div>
						<div class="form-group ">

							<label for="New Vpn">New Vpn</label>
							<input type="text"  class="form-control" name="new_VPN"  id="new_VPN" placeholder="New Vpn" />
						</div>
					</form>			
				</div>
			</div>
		</section>
	</article>
</div>