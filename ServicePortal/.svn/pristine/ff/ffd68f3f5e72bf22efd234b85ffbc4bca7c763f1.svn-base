  import { NsoService } from '../nso.service';

  export class Topologycall {
 private arr=[];
	constructor(private nsoService:NsoService){

	}
	getData(name,path){
  	const URL = {
		  'URL': path

	  }
	  //this.arr=[];
	  let element={};
		 this.nsoService.nsoServicesSchemaGetValue(URL).subscribe(data => {
		 	console.log(data);
		 	console.log("_________________________fg_"+path+"  "+name);
		  	if(data.result!=null){

		  		element[name]=data.result.value;
		  	}else{
		  		element[name]="data not found";
		  	}
		  	this.arr.push(element);
			});  
	

       return this.arr;
  }
	getDataquery(name,path,xpath,selection){
  	const URL = {
		  'context': path,
		  'xpath':xpath,
		  'selection':selection
	  }
	  //this.arr=[];
	  let element={};
		 this.nsoService.nsoDataquery(URL).subscribe(data => {
		 	console.log(data);
		 	console.log("_________________________fg_"+path+"  xpath==============="+xpath);
		  	if(data.result!=null){
		  		element[name]=data.result.results;
		  		
		  	}else{
		  		element[name]="data not found";
		  	}
		  	this.arr.push(element);
			});  
	

       return this.arr;
  }

}
