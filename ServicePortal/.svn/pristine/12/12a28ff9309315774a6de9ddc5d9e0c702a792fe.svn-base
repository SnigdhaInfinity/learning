@font-face {
    font-family: OpenSans-Regular;
    src: url("assets/fonts/OpenSans-Regular.ttf");
}

@font-face {
    font-family: OpenSans-Semibold;
    src: url("assets/fonts/OpenSans-Semibold.ttf");
}

body {
    
    font-family: "OpenSans-Regular", sans-serif;
    font-size: 13px;
    font-weight: normal;
    background: #fff;
    color:#5a5858;
}

html, body,.login {
    height: 100%;
    min-height: 100%;
}
.login{background:#d0cece;}
.loginBlock input:-webkit-autofill, .loginBlock input:-webkit-autofill:hover,
 .loginBlock input:-webkit-autofill:focus, 
 .loginBlock input:-webkit-autofill:active {
    transition: background-color 5000s ease-in-out 0s;   
}



input[type=checkbox], input[type=radio] {
    margin: 2px 3px 6px;
}
.form-control,.nav > li > a,.owl-dt-calendar-table .owl-dt-calendar-header .owl-dt-weekdays th,
.owl-dt-calendar-table .owl-dt-calendar-cell-content,
.owl-dt-container-info .owl-dt-container-range{
    font-size:13px !important;}

h1 {
    color: #1a4766;
    margin: 29px 0 35px 0;
    border-bottom: 1px solid #e6e8ea;
    width: 100%;
    font-size: 20px;
    font-family: "OpenSans-Semibold", sans-serif;
}

h2 {
    font: 18px "OpenSans-Semibold", sans-serif;
    color: #d69926;
    margin: 16px 0 25px 16px;
    width: 80%;

}

h5{
   font: normal 18px "Calibri", sans-serif;
    color: #0cb8d6;
    margin: 16px 0 25px 16px;
    width: 97%;
    text-transform: capitalize;
    background: #e6f3f3;
    padding: 5px 10px;
    border-left: 3px solid;
}


:focus,.btn.active.focus, .btn.active:focus,
 .btn.focus, .btn:active.focus, .btn:active:focus, .btn:focus {
    outline: none;
}

a {
    color: #3ea70a;
    text-decoration: none;
}

a:focus, a:hover {
    text-decoration: none;
    outline: #e8e5e5;
}

.jsonButton {
    text-align: center;
    clear: both;
}

.SchoiceDiv > select {
    width: 200px;
}

.n-popupContainer {
    z-index: 9999;
}

.n-topology-tooltip {
    opacity: 1 !important;
}

