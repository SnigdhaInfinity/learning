.CssAmit {
    max-width: 900px !important;
}

#topology {
    width: 875px;
    height: 500px;
}

.cdk-overlay-container {
    z-index : 2000;
}
 
 .CssAmit .ng-star-inserted{ 
    border: 1px solid #d0cbcb;
    background: #f1f1f1;
    padding: 10px;
}
.rowHeader{-webkit-border-radius: 4px; 
  -moz-border-radius: 4px; 
  border-radius:4px; 
   background:#234f69;
   border:1px solid gray;
  }
 table.dataTable.no-footer{border-bottom:none;}  

 .CssAmit .ng-star-inserted td{border:1px solid #bdbbbb;    background: #fff;}

.rowHeader th{
       border-right: 1px solid #a09e9e;
    color: #fff;
    text-transform: capitalize;
    font-size:12px;
    font-size: 13px;
    padding: 10px 18px !important;
}
.rowHeader th:last-child{border-right:none;}

.panel-group .panel{margin-bottom: 5px;}

.panel-title a {
    color: #69719e;
    font-size: 14px;
    text-transform: capitalize;
    font-family: OpenSans-Semibold;
}

 .panel-title .caret{
    float: right;
    margin-top: 8px;
    color: #2356cc;
}

  .panel-body{
   max-height: 200px;
    overflow-y: auto;
}

 .glyphicon.glyphicon-plus{
    color: #428a0e;
    font-size: 12px; 
    margin-top:2px;  
}
table.dataTable thead .sorting{
    text-transform: capitalize;
}

.sorting_1 a{    
    float: left;
    width: auto;
    margin-right: 2px;}

.ng-star-inserted .sorting_1 br{
    display:none;
}

.glyphicon.glyphicon-chevron-right[_ngcontent-c5]{
    font-size:11px;
    top:3px;
}
.tabData{ margin: 0 auto 60px;}

.tabData ul li a{
    font-family: "OpenSans-Semibold",sans-serif;
    font-size: 13px !important;
}
.glyphicon.glyphicon-chevron-right{
    top: 3px;
    font-size: 11px;
}

/*-- Popup css --*/
.dialog{border-top: 5px solid #0db1e2;}
.dialog .rowHeader th{ background:#164388;}
.btn-success,.btn-success[disabled] {
    background: #66b51e;
    color: #fff!important;
    opacity: unset !important;
}
.btn-danger{background:#e63b08;}

.btn {
    padding: -0.16rem 1.14rem;
    font-size: 12px;
    -webkit-transition: all .2s ease-in-out;
    transition: all .2s ease-in-out;
    margin: .375rem;
    border: 0;
    border-radius: .125rem;
    cursor: pointer;
    text-transform: uppercase;
    white-space: normal;
    word-wrap: break-word;
    color: #fff!important;
        opacity: unset;
}

.btn-success:hover,.btn-success[disabled]:hover{background:#80ce39;}
 .dialog h1{ margin: 21px 0 12px 0;}

 .dialog h2 {
    font: 17px "OpenSans-Semibold", sans-serif;
    color: #4888bb;
    margin: 16px 0 17px 0px;
    width: 80%;
}
